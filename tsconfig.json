{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@mui/material": ["./node_modules/@mui/joy"]
    },
    "target": "es6",
    "outDir": "dist",
    "jsx": "react",
    "removeComments": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["vitest/globals", "@figma/plugin-typings"],
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
